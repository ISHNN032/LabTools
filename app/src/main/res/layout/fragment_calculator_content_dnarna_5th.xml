<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FunctionFragment.DnarnaTabFragment.DnarnaFifthBtnFragment">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:paddingTop="20dp">

    <View
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:background="#111111" />

    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_marginTop="2dp"
        android:background="#111111" />

    <TextView
        android:id="@+id/dnarna_5th_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/colorFunctionBackground"
        android:gravity="center"
        android:padding="5dp"
        android:text="@string/dnarna_fragment_5thbtn_text_kor"
        android:textSize="20dp" />

    <View
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:background="#111111" />

    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_marginTop="2dp"
        android:background="#111111" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/colorFunctionBackground"
        android:orientation="vertical">
        <ImageView
            android:id="@+id/dnarna_5th_table"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="2dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:src="@drawable/s_10_5_table" />

    </LinearLayout>

    </LinearLayout>
</FrameLayout>