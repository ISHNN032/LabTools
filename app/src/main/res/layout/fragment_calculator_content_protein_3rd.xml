<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.home.content.ProteinTabFragment.ProteinThirdBtnFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:paddingTop="20dp">

    <View
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:background="#111111" />

    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_marginTop="2dp"
        android:background="#111111" />

    <TextView
        android:id="@+id/cell_3rdbtn_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/colorFunctionBackground"
        android:gravity="center"
        android:padding="5dp"
        android:text="@string/protein_fragment_3rdbtn_text_kor"
        android:textSize="20dp" />

    <View
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:background="#111111" />

    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_marginTop="2dp"
        android:background="#111111" />

    <com.github.chrisbanes.photoview.PhotoView
        android:id="@+id/imageView2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="2dp"
        android:src="@drawable/s_9_3_table" />

</LinearLayout>
</FrameLayout>